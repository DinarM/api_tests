{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "type": "object",
    "properties": {
      "id": {
        "type": "integer"
      },
      "year_id": {
        "type": "integer"
      },
      "user_group_id": {
        "type": "integer"
      },
      "read": {
        "type": "boolean"
      },
      "write": {
        "type": "boolean"
      },
      "user_group_name": {
        "type": "string"
      },
      "field_id": {
        "type": "integer"
      },
      "field_name": {
        "type": "string"
      },
      "spec_id": {
        "type": "integer"
      },
      "year": {
        "type": "integer"
      },
      "users": {
        "type": "array",
        "items": [
          {
            "type": "object",
            "properties": {
              "user_id": {
                "type": "integer"
              },
              "username": {
                "type": "string"
              },
              "role": {
                "type": "string"
              },
              "company_id": {
                "type": "integer"
              },
              "company_name": {
                "type": "string"
              }
            },
            "required": [
              "user_id",
              "username",
              "role",
              "company_id",
              "company_name"
            ]
          }
        ]
      }
    },
    "required": [
      "id",
      "year_id",
      "user_group_id",
      "read",
      "write",
      "user_group_name",
      "field_id",
      "field_name",
      "spec_id",
      "year",
      "users"
    ]
  }